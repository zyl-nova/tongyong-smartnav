.cs-selector-mask.cs-selector-mask_hidden {
  opacity: 0;
  transform: scale3d(1, 0, 1);
}
.cs-selector-mask {
  background-color: rgba(0, 0, 0, 0.4);
  height: 100vh;
  left: 0;
  opacity: 1;
  position: fixed;
  top: 0;
  transform: scale3d(1, 1, 1);
  transition: all 0.3s;
  width: 100vw;
  z-index: 9999;
}
.cs-selector-container {
  background-color: #fff;
  border-top-left-radius: 12px;
  border-top-right-radius: 12px;
  bottom: 0;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  height: 75%;
  left: 0;
  position: absolute;
  width: 100%;
}
.cs-title-area {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex: none;
  height: 56px;
  justify-content: space-between;
  padding: 0 24rpx;
}
.cs-title-text {
  color: #000;
  font-size: 16px;
  font-weight: 500;
  line-height: 16px;
}
.cs-title-icon {
  height: 16px;
  padding: 8rpx;
  width: 16px;
}
.cs-hidden-icon {
  visibility: hidden;
}
.cs-tab-container {
  display: -webkit-flex;
  display: flex;
  flex: none;
  height: 32px;
}
.cs-divider-line {
  background-color: #e5e5e5;
  flex: none;
  height: 1px;
  transform: scaleY(0.5);
  transform-origin: 0 0;
}
.cs-tab-container {
  padding: 0 32rpx;
}
.cs-tab-label {
  box-sizing: border-box;
  height: 32px;
  margin-right: 56rpx;
  padding-bottom: 14px;
}
.cs-unselected-tab {
  border-bottom: 1px solid #0062ff;
  color: #0062ff;
  font-size: 14px;
  padding-left: 6rpx;
  padding-right: 6rpx;
}
.cs-selected-tab {
  border-bottom: 1px solid transparent;
  color: #333;
  font-size: 16px;
  font-weight: 500;
  margin-top: -2px;
}
.cs-loading-container {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex: auto;
  flex-direction: column;
  justify-content: center;
}
@-webkit-keyframes rotation {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(1turn);
  }
}
@keyframes rotation {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(1turn);
  }
}
.cs-loading-icon {
  animation: rotation 1.5s linear infinite;
  height: 45px;
  margin-bottom: 11px;
  margin-top: -20%;
  width: 45px;
}
.cs-error-container {
  align-items: center;
  display: -webkit-flex;
  display: flex;
  flex: auto;
  flex-direction: column;
  justify-content: center;
}
.cs-error-icon {
  height: 103px;
  margin-top: -30%;
  width: 103px;
}
.cs-error-text,
.cs-loading-text {
  color: #333;
  font-size: 16px;
  font-weight: 500;
  line-height: 16px;
  margin-top: 8px;
  opacity: 0.7;
  text-align: center;
}
.cs-scroll-container {
  flex: auto;
  height: calc(100% - 89px);
  overflow: hidden;
}
.cs-list-container {
  padding: 0 30rpx 0 32rpx;
}
.cs-hotcity-container {
  margin-bottom: 8px;
  margin-top: 12px;
}
